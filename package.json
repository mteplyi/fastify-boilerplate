{
  "name": "fastify-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --exit-child --rs --watch .env src/main.ts",
    "prebuild": "rimraf dist/",
    "build": "npm run prebuild && tsc --project tsconfig.build.json",
    "start": "node dist/src/main.js",
    "dotenv:gen-example": "ts-node cli/gen-env-example.cli.ts .env.example",
    "swagger:gen-doc": "ts-node cli/gen-swagger-doc.cli.ts",
    "codegen:github-graphql": "gql-gen --config github-graphql.codegen.yaml",
    "lint:staged:pretty-quick": "pretty-quick --staged",
    "lint:staged:eslint": "echo && lint-staged",
    "lint:all:eslint": "eslint --max-warnings=0 '**/*.ts'",
    "lint:all:tsc": "tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "npm-run-all --silent --sequential lint:staged:*",
    "pre-push": "npm-run-all --silent --continue-on-error --parallel lint:all:* 'swagger:gen-doc -- --bail'"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-document-nodes": "2.3.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-resolvers": "2.7.2",
    "@octokit/graphql-schema": "^10.74.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/flat": "^5.0.2",
    "@types/node": "^18.6.3",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "json-schema-to-ts": "^2.5.5",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "npm-run-all2": "^6.0.1",
    "pino-caller": "^3.2.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@fastify/helmet": "^9.1.0",
    "@fastify/rate-limit": "^7.3.0",
    "@fastify/sensible": "^5.1.0",
    "@fastify/swagger": "^7.4.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.0.0",
    "ajv": "^8.11.0",
    "camelcase": "^6.3.0",
    "cls-rtracer": "^2.6.2",
    "deepmerge-ts": "^4.2.1",
    "dotenv": "^16.0.1",
    "env-schema": "^5.0.0",
    "fastify": "^4.3.0",
    "flat": "^5.0.2",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.2.2",
    "pino": "^8.3.1",
    "statuses": "^2.0.1",
    "tsconfig-paths": "^4.0.0"
  }
}
